/* pages/start-exam/start-exam.wxss */
.start-exam{
  width: 100%;
  height: 100%;
  background:url(https://free.liaoningdoupo.com/files/pk-bg.png) no-repeat 0 0;
  background-size:100% 100%;
  position: relative;
}
.start-box{
  position: absolute;
  top:50%;
  transform: translateY(-50%);
  left:50%;
  transform: translateX(-50%);

}
.tranform{
  -webkit-animation:myfirst1 5s linear infinite;
}
.start-img{
  width: 340rpx;
  height: 340rpx;
  position: absolute;
  left:50%;
  top:50%;
  bottom:50%;
  right:50%;
  margin-left:-170rpx;
  margin-top:-170rpx;
}
.start-img2{
  opacity: 0.6;
}
.line{
  width:405rpx;
  height: 250rpx;
  position: absolute;
  left:50%;
  top:50%;
  bottom:50%;
  right:50%;
  margin-left:-220rpx;
  margin-top:-211rpx;

}
@-webkit-keyframes myfirst1 {
	0% {
	transform:rotate(0deg);
}
50% {
	transform:rotate(180deg);
}
100% {
	transform:rotate(360deg);
}
}
.start-logo{
  width:50%;
  height: 50rpx;
  position: absolute;
  bottom:150rpx;
  left: 50%;
  transform: translateX(-50%);
  background: url('data:image/png;base64,') no-repeat 0 0;
  background-size:100% 100%;
}
.red-line{
  background:url('data:image/png;base64,') no-repeat 0 0;
  background-size: 100% 100%;
}
.blue-line{
  height: 280rpx;
  margin-top:-66rpx;
  margin-left:-183rpx;
  background:url('data:image/png;base64,') no-repeat 0 0;
  background-size:100% 100%;
}