/* pages/new-date/new-date.wxss */
.new-date{
  width: 100%;
  height: 100%;
  background:url(https://free.liaoningdoupo.com/files/bg-x.png) no-repeat 0 0;
  background-size:100%;
  overflow:auto;
  /* padding:20rpx 20rpx; */
}
.list-item{
  width: 92%;
  height: 250rpx;
  
  
  color:#fff;
  font-size: 28rpx;
  margin:20rpx auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}
.list-item{
  background:url('data:image/png;base64,') no-repeat 0 0;
  background-size:100% 100%;
}
.list-item:first-child{
  background:url('data:image/png;base64,') no-repeat 0 0;
  
}
.num{
  position: absolute;
  left:10rpx;
  top: 10rpx;
}
.user-info{
  display: flex;
  flex-direction: column;
}
.item-r{
  width:80%;
}
.item-l{
  margin-left:40rpx;
  width:120rpx;
  height: 120rpx;
}
.info-t{
  display: flex;
  justify-content: space-between;
  border-bottom:1px solid #fff;
  padding-bottom:20rpx;
  padding-right: 40rpx;
  padding-left:20rpx
}
.user-name image{
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}
.user-font{
  font-size: 8px;
  display: inline-block;
  margin-left: 5rpx;
}
.info-b{
  display: flex;
  justify-content: space-between;
  padding:30rpx;
}
.tz{
  width: 150rpx;
  height: 40rpx;
  line-height: 40rpx;
  background:rgba(0,0,0,.3);
  border-radius: 25px;
  text-align: left;
  padding-left: 30rpx;
  font-size: 8px;
}
.tz image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  vertical-align: middle;
  margin-bottom:7rpx;
}
.logo{
  width: 70%;
  height: 70rpx;
  margin:60rpx auto;
}
.title{
  color:#444;
  padding-bottom:20rpx;
  border-bottom:1px solid #ccc;
  font-weight: 600;
  text-align: center;
}
.user{
  display:flex;
  justify-content: space-between;
  color:#444444;
  font-size: 30rpx;
  line-height: 30rpx;
  padding:30rpx 0;
}
.user-info2{
  display: flex;
  justify-content: flex-start;

}
.user-name2 text{
  display:block;
  margin-top:10rpx;
}
.user-name2 text:last-child{
  font-size:20rpx;
}
.user-header{
  width:80rpx;
  height: 80rpx;
  margin-right: 10rpx;
}
.time{
  margin-top:30rpx;
}
.info{
  color:#191919;
  font-size:30rpx;
  /* margin-left: 40rpx; */
  text-align: center;
}
.pk-radio{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size:28rpx;
  color:#1f1f1f;
  margin:40rpx 0 60rpx;
}
.popup-btns{
  display: flex;
  justify-content: space-between;
}
.cancel{
  display: flex;
  justify-content: center;
}
.popup-btns view,.cancel view{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 25px;
  font-size: 30rpx;
  margin-bottom:30rpx;
}
.no-pk{
  border:1px solid #ccc;
  color:#191919;
}
.go-pk{
  background: #b53f38;
  color:#fff;
}